{
    "name": "hive-builder template container",
    "dockerFile": "Dockerfile",
    "overrideCommand": false,
    "privileged": true,
    "customizations": {
        "vscode": {
            "extensions": [
                "redhat.ansible"
            ],
            "settings": {
                "python.defaultInterpreterPath": "/opt/hive/bin/python",
                "python.terminal.activateEnvInCurrentTerminal": true
            }
        }
    }
}